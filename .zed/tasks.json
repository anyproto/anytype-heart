[
  {
    "label": "Run gRPC Server",
    "command": "go",
    "args": [
      "run",
      "-tags=nomutexdeadlockdetector,noauth,nographviz,appdebug",
      "./cmd/grpcserver"
    ],
    "env": {
      "ANYDEBUG": ":6061",
      "ANYPROF": ":6060",
      "ANYTYPE_GATEWAY_ADDR": "127.0.0.1:31006",
      "ANYTYPE_GRPC_ADDR": "127.0.0.1:31007",
      "ANYTYPE_GRPCWEB_ADDR": "127.0.0.1:31008",
      "ANYTYPE_API_DEBUG": "1",
      "CC": "/usr/bin/cc",
      "CXX": "/usr/bin/c++",
      "CGO_CFLAGS": "-Wno-deprecated-declarations -Wno-deprecated-non-prototype -Wno-xor-used-as-pow"
    },
    "use_new_terminal": true
  },
  {
    "label": "Run gRPC Server Custom",
    "command": "go",
    "args": [
      "run",
      "-tags=nomutexdeadlockdetector,noauth,nographviz,appdebug,envnetworkcustom",
      "./cmd/grpcserver"
    ],
    "env": {
      "ANYDEBUG": ":6061",
      "ANYPROF": ":6060",
      "ANYTYPE_GATEWAY_ADDR": "127.0.0.1:31006",
      "ANYTYPE_GRPC_ADDR": "127.0.0.1:31007",
      "ANYTYPE_GRPCWEB_ADDR": "127.0.0.1:31008",
      "ANYTYPE_API_DEBUG": "1",
      "CC": "/usr/bin/cc",
      "CXX": "/usr/bin/c++",
      "CGO_CFLAGS": "-Wno-deprecated-declarations -Wno-deprecated-non-prototype -Wno-xor-used-as-pow"
    },
    "use_new_terminal": true
  }
]
